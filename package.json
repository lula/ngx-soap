    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.4.5",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tsickle": "^0.34.0",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
