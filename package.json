    "@angular/platform-browser-dynamic": "^7.1.3",
    "@angular/router": "^7.1.3",
    "buffer": "^5.4.3",
    "concat-stream": "^2.0.0",
    "core-js": "^3.6.4",
    "crypto-js": "^4.0.0",
    "events": "^3.1.0",
    "lodash": "^4.17.15",
    "rxjs": "^6.5.3",
    "sax": "^1.2.4",
    "stream": "0.0.2",
    "uuid": "^3.4.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.3",
